{
    "extends": "airbnb",
    "rules": {
        "indent": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "no-alert": ["off"],
        "no-console": ["off"],
        "no-param-reassign": ["error", { "props": false }],
        "no-underscore-dangle": ["error", { "allow": ["_id", "_metadata"] }]
    }
}